    "import SnakeEnv\n",
    "\n",
    "env = SnakeEnv()\n",
    "\n",
    "for episode in range(5):\n",
    "    state = env.reset()\n",
    "    done = False\n",
    "    while not done:\n",
    "        action = env.action_space.sample()  # Action al√©atoire\n",
    "        state, reward, done, info = env.step(action)\n",
    "        env.render()\n"
   ]
